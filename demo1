class demo1 
{
    int num1,num2,ans;//instance variable
    
    void init()//non-parametrized method
    {
        num1=5;//body of method
       num2=6;//local variable
    }
    void calc()
    {
        ans=num1+num2;
    }
    
    void display()
    {
     System.out.println("the result is:"+ans);
    }
    }
