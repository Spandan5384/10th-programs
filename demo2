class demo2
{
    int n1,n2,ans;
    void init(int num1,int num2)//parametrized method
    {
        n1=num1;
        n2=num2;
    }
    void calc()
    {
        ans=n1+n2;//adding two numbers
    }
    void display()
    {
        System.out.println(ans);
    }
    public static void main()
    {
        demo2 obj=new demo2();
        obj.init(4,6);//calling statement
        obj.calc();
        obj.display();
    }
}
        
