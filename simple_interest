class simple_interest
{
    double principle,time,rate,interest;
    
    void init()
    {
        principle=2000.0;
        time=5.0;
        rate=2.5;
    }
    void calc()
    {
        interest=(principle*rate*time)/100;
       
    }
    void display()
    {
        System.out.println("The amount of interest is "+interest);
    }
}
